<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-soft-img-pick" version="1.0.0" 
    xmlns="http://apache.org/cordova/ns/plugins/1.0" 
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>softImgPick</name>
    <js-module name="softImgPick" src="www/softImgPick.js">
        <clobbers target="cordova.plugins.softImgPick" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="softImgPick">
                <param name="android-package" value="cordova.plugin.soft.img.pick" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/softImgPick.java" target-dir="src/cordova/plugin/soft/img/pick" />
    </platform>
</plugin>